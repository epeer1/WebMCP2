{
  "name": "webmcp-instrument-runtime",
  "version": "0.1.1",
  "description": "Browser Runtime for WebMCP Auto-Instrumentation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "clean": "rimraf dist *.tsbuildinfo",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "rimraf": "^6.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "keywords": [
    "webmcp-instrument",
    "mcp",
    "ai-agent",
    "browser-tools"
  ],
  "license": "MIT",
  "author": "webmcp-instrument",
  "repository": {
    "type": "git",
    "url": "https://github.com/epeer1/WebMCP2"
  }
}